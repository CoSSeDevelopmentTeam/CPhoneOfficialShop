<?xml version="1.0" encoding="UTF-8" ?>
<!--<!DOCTYPE manifest SYSTEM "https://raw.githubusercontent.com/CoSSeDevelopmentTeam/CPhone/master/CPhoneManifest.dtd">-->

<manifest>
    <application>
        <title>OfficialShop</title>
        <description>公式ショップアプリ。アイテムを買うだけでなく売ることでお金を手に入れられます！</description>
        <author>popkechupki</author>
        <version>1.2.0</version>
        <mcbeversion>1.14</mcbeversion>
        <iconType>url</iconType>
        <icon>https://info.comorevi.net/images/favicon/logo_transparent.png</icon>
        <initialize>net.comorevi.cpapp.shop.EventListener</initialize>
    </application>

    <permission type = "default" />

    <activity>
        <name>MainActivity</name>
        <main>net.comorevi.cpapp.shop.MainActivity</main>
    </activity>

    <external-lang>
        <lang region="en">
            <title>OfficialShop</title>
            <description>This is a official shop application for cphone. You can not only buy items but sell your items.</description>
        </lang>
        <lang region="ja">
            <title>公式ショップ/OfficialShop</title>
            <description>公式ショップアプリ。アイテムを買うだけでなく売ることでお金を手に入れられます！</description>
        </lang>
    </external-lang>
</manifest>